        "createdTime": "2023-09-15T12:59:20.3950951Z",
        "changedTime": "2023-09-15T13:41:50.1332794Z",
        "api": {
          "name": "[parameters('azureFileConnectionName')]",
          "displayName": "Azure File Storage",
